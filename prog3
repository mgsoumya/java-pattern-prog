class HelloWorld {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
       int n = scan.nextInt();
       int i;
       int j;
       for(i=1; i<=n; i++){
           for(int k=1; k<=n-i; k++){
               System.out.print(" ");
           }
           for(j=1; j<=i; j++){
               System.out.print("*");
           }
           System.out.println();
       }
    }
}
